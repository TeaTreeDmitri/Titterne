{
  "expo": {
    "name": "titterne",
    "slug": "titterne",
    "owner": "wifiklubben",
    "version": "1.1.4",
    "orientation": "landscape",
    "icon": "./assets/TitterneIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/TitterneSplash.png",
      "resizeMode": "contain",
      "backgroundColor": "#8AC1DF"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.teatreedimitri.titterne",
      "config": {
        "usesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.teatreedimitri.titterne"
    },
    "plugins": [
      [
        "expo-screen-orientation",
        {
          "initialOrientation": "LANDSCAPE"
        }
      ]
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e8fa652f-2c93-49c5-9355-4582bcdfeac7"
      }
    }
  }
}
